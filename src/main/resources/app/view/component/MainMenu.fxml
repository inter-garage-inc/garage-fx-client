<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="app.controller.component.MainMenuController">

    <Button fx:id="buttonCheckIn" onAction="#handleOnActionButtonCheckIn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false"  styleClass="button-menu, button-blue-menu" text="Check In" />
    <Button fx:id="buttonCheckout" onAction="#handleOnActionButtonCheckout" layoutX="158.0" layoutY="14.0" mnemonicParsing="false"  styleClass="button-menu, button-blue-menu" text="Checkout" />
    <Button fx:id="buttonMonthly" onAction="#handleOnActionButtonMonthly" layoutX="301.0" layoutY="14.0" mnemonicParsing="false"  styleClass="button-menu, button-blue-menu" text="Mensalistas" />
    <Button fx:id="buttonOpenAccounts" onAction="#handleOnActionButtonOrdersOpen" layoutX="444.0" layoutY="14.0" mnemonicParsing="false"  styleClass="button-menu, button-blue-menu" text="Contas Abertas" />
    <Button fx:id="buttonVacancyMap" onAction="#handleOnActionButtonVacancyMap" layoutX="587.0" layoutY="14.0" mnemonicParsing="false" styleClass="button-menu, button-blue-menu" text="Mapa de Vagas" />
