<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<AnchorPane prefHeight="720.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controller.PageMenuController">

    <stylesheets>
        <URL value="@css/styles.css" />
    </stylesheets>
    <Button fx:id="btnCheckIn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#checkIn" styleClass="button-menu, button-blue-menu" text="Check In" />
    <Button fx:id="btnCheckout" layoutX="158.0" layoutY="14.0" mnemonicParsing="false" onAction="#checkout" styleClass="button-menu, button-blue-menu" text="Checkout" />
    <Button fx:id="btnMonthly" layoutX="301.0" layoutY="14.0" mnemonicParsing="false" onAction="#monthly" styleClass="button-menu, button-blue-menu" text="Mensalistas" />
    <Button fx:id="btnOpenAccounts" layoutX="444.0" layoutY="14.0" mnemonicParsing="false" onAction="#openAccounts" styleClass="button-menu, button-blue-menu" text="Contas Abertas" />
    <Button fx:id="btnVanacyMap" layoutX="587.0" layoutY="14.0" mnemonicParsing="false" onAction="#vanacyMap" styleClass="button-menu, button-blue-menu" text="Mapa de Vagas" />
</AnchorPane>
