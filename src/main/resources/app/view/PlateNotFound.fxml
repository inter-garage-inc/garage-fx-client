<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import java.net.URL?>
<AnchorPane prefHeight="200.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controller.PlateNotFoundController">
   <stylesheets>
      <URL value="@css/styles.css" />
   </stylesheets>

   <FlowPane layoutX="100.0" layoutY="30.0" prefHeight="100.0" prefWidth="300.0">
      <Label prefWidth="300.0" styleClass="field-label-form" text="Tente novamente ou verifique a aba “Contas Abertas”." textAlignment="CENTER" wrapText="true" />
   </FlowPane>

   <Button fx:id="btnClose" layoutX="64.0" layoutY="144.0" mnemonicParsing="false" onAction="#closeForm" prefHeight="26.0" prefWidth="100.0" styleClass="button-blue" text="Fechar" />
   <Button fx:id="btnOpenAccounts" layoutX="250.0" layoutY="144.0" mnemonicParsing="false" onAction="#openAccounts" prefHeight="26.0" prefWidth="181.0" styleClass="button, button-green" text="Contas Abertas" />
</AnchorPane>
